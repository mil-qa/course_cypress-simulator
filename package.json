  "license": "MIT",
  "type": "commonjs",
  "devDependencies": {
    "axe-core": "^4.10.2",
    "cypress": "^14.2.1",
    "cypress-axe": "^1.6.0",
    "cypress-split": "^1.24.14"
  }
}
